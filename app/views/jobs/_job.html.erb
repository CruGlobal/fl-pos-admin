<tr class="cycle('', 'bg-gray-50') %>">
  <td class="p-4 text-sm font-normal text-gray-900 whitespace-nowrap"><%= job.type %>
    <style>
        .recordrow {top: -75px; position: relative;}
    </style>
    <a id="record<%= job.id %>" class="recordrow"></a>
  </td>
  <td class="p-4 text-sm font-normal text-gray-900"><%= job.status %></td>
  <td class="p-4 text-sm font-normal text-gray-900"><%= job.updated_at %></td>
  <% job.logs.order(updated_at: :desc).limit(10).each do |log| %>
    </tr><tr>
    <td></td>
    <td></td>
    <td></td>
    <td class="p-4 text-sm font-normal text-gray-900"><%= log.content %></td>
    <td class="p-4 text-sm font-normal text-gray-900"><%= log.updated_at %></td>
  <% end %>
  <td class="p-4 text-sm font-normal text-gray-900">
    <%= link_to('Edit This Job', edit_job_path(job), class: "bg-green-100 text-green-800 text-xs font-medium mr-2 px-2.5 py-0.5 rounded-md dark:bg-gray-700 dark:text-green-400 border border-green-100 dark:border-green-500")%>
  </td>
</tr>
